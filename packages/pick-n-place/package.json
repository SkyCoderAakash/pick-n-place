{
  "name": "react-pickplace",
  "version": "1.0.2",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "files": ["dist"],
  "keywords": [
    "react",
    "reorder",
    "list",
    "sortable",
    "pick",
    "drag",
    "ui"
  ],
  "description": "A dragless pick-and-place reorderable list component for React",
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "@types/react": "^19.2.7",
    "rollup": "^4.54.0",
    "rollup-plugin-esbuild": "^6.2.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "rollup-plugin-dts": "^6.1.0",
    "typescript": "^5.9.3"
  }
}
